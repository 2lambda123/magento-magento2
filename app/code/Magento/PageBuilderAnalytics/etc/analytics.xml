<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Analytics:etc/analytics.xsd">
    <file name="pagebuilder_page">
        <providers>
            <customProvider name="pagebuilder_page" class="Magento\PageBuilderAnalytics\Model\CmsPageProvider" />
        </providers>
    </file>
    <file name="pagebuilder_block">
        <providers>
            <customProvider name="pagebuilder_block" class="Magento\PageBuilderAnalytics\Model\CmsBlockProvider" />
        </providers>
    </file>
    <file name="pagebuilder_category">
        <providers>
            <customProvider name="pagebuilder_category" class="Magento\PageBuilderAnalytics\Model\CatalogCategoryProvider" />
        </providers>
    </file>
    <file name="pagebuilder_product_attributes">
        <providers>
            <customProvider name="pagebuilder_product_attributes" class="Magento\PageBuilderAnalytics\Model\CatalogProductProvider" />
        </providers>
    </file>
</config>
