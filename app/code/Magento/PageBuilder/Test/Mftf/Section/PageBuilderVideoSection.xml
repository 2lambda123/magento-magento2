<sections xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="../../../../../../../dev/tests/acceptance/vendor/magento/magento2-functional-testing-framework/src/Magento/FunctionalTestingFramework/Page/etc/SectionObject.xsd">
    <section name="VideoOnStage">
        <!-- Empty Video -->
        <element name="empty" type="text" selector="(//div[contains(@class,'pagebuilder-video')])[{{arg1}}][not(descendant::iframe)]" parameterized="true"/>
        <element name="icon" type="text" selector="(//div[contains(@class,'pagebuilder-video')])[{{arg1}}]//i[@class='icon-pagebuilder-video']" parameterized="true"/>
        <element name="text" type="text" selector="(//div[contains(@class,'pagebuilder-video')])[{{arg1}}]//span[.='{{arg2}}']" parameterized="true"/>
        <!-- General Configuration -->
        <element name="base" type="text" selector="(//div[contains(@class,'pagebuilder-video')]//iframe)[{{arg1}}]" parameterized="true"/>
        <element name="source" type="text" selector="(//div[contains(@class,'pagebuilder-video')]//iframe)[{{arg1}}][@src='{{arg2}}']" parameterized="true"/>
        <element name="height" type="text" selector="(//div[contains(@class,'pagebuilder-video')]//iframe)[{{arg1}}][contains(@style,'height: {{arg2}};')]" parameterized="true"/>
        <element name="noHeight" type="text" selector="(//div[contains(@class,'pagebuilder-video')]//iframe)[{{arg1}}][not(contains(@style,'height:'))]" parameterized="true"/>
        <element name="width" type="text" selector="(//div[contains(@class,'pagebuilder-video')]//iframe)[{{arg1}}][contains(@style,'width: {{arg2}};')]" parameterized="true"/>
        <element name="noWidth" type="text" selector="(//div[contains(@class,'pagebuilder-video')]//iframe)[{{arg1}}][not(starts-with(@style,'width:')) and not(contains(@style,' width:'))]" parameterized="true"/>
        <!-- Advanced Configuration -->
        <element name="alignment" type="text" selector="(//div[contains(@class,'pagebuilder-video')]//iframe)[{{arg1}}][contains(@style,'text-align: {{arg2}};')]" parameterized="true"/>
        <element name="noAlignment" type="text" selector="(//div[contains(@class,'pagebuilder-video')]//iframe)[{{arg1}}][not(contains(@style,'text-align:'))]" parameterized="true"/>
        <element name="border" type="text" selector="(//div[contains(@class,'pagebuilder-video')]//iframe)[{{arg1}}][contains(@style,'border-style: {{arg2}};')]" parameterized="true"/>
        <element name="borderColor" type="text" selector="(//div[contains(@class,'pagebuilder-video')]//iframe)[{{arg1}}][contains(@style,'border-color: {{arg2}};')]" parameterized="true"/>
        <element name="noBorderColor" type="text" selector="(//div[contains(@class,'pagebuilder-video')]//iframe)[{{arg1}}][not(contains(@style,'border-color:'))]" parameterized="true"/>
        <element name="borderWidth" type="text" selector="(//div[contains(@class,'pagebuilder-video')]//iframe)[{{arg1}}][contains(@style,'border-width: {{arg2}}px;')]" parameterized="true"/>
        <element name="borderRadius" type="text" selector="(//div[contains(@class,'pagebuilder-video')]//iframe)[{{arg1}}][contains(@style,'border-radius: {{arg2}}px;')]" parameterized="true"/>
        <element name="cssClasses" type="text" selector="(//div[contains(@class,'pagebuilder-video')]//iframe)[{{arg1}}][contains(@class,'{{arg2}}')]" parameterized="true"/>
        <element name="noCssClasses" type="text" selector="(//div[contains(@class,'pagebuilder-video')]//iframe)[{{arg1}}]" parameterized="true"/>
        <element name="margins" type="text" selector="(//div[contains(@class,'pagebuilder-video')]//iframe)[{{arg1}}][contains(@style,'margin: {{arg2}}px;')]" parameterized="true"/>
        <element name="padding" type="text" selector="(//div[contains(@class,'pagebuilder-video')]//iframe)[{{arg1}}][contains(@style,'padding: {{arg2}}px;')]" parameterized="true"/>
    </section>
    <section name="VideoOnStorefront">
        <!-- Empty Video -->
        <element name="empty" type="text" selector="(//div[@data-role='video']//iframe)[{{arg1}}][@src='']" parameterized="true"/>
        <!-- General Configuration -->
        <element name="base" type="text" selector="(//div[@data-role='video']//iframe)[{{arg1}}]" parameterized="true"/>
        <element name="source" type="text" selector="(//div[@data-role='video']//iframe)[{{arg1}}][@src='{{arg2}}']" parameterized="true"/>
        <element name="height" type="text" selector="(//div[@data-role='video']//iframe)[{{arg1}}][contains(@style,'height: {{arg2}}')]" parameterized="true"/>
        <element name="noHeight" type="text" selector="(//div[@data-role='video']//iframe)[{{arg1}}][not(contains(@style,'height:'))]" parameterized="true"/>
        <element name="width" type="text" selector="(//div[@data-role='video']//iframe)[{{arg1}}][contains(@style,'width: {{arg2}}')]" parameterized="true"/>
        <element name="noWidth" type="text" selector="(//div[@data-role='video']//iframe)[{{arg1}}][not(starts-with(@style,'width:')) and not(contains(@style,' width:'))]" parameterized="true"/>
        <!-- Advanced Configuration -->
        <element name="alignment" type="text" selector="(//div[@data-role='video']//iframe)[{{arg1}}][contains(@style,'text-align: {{arg2}};')]" parameterized="true"/>
        <element name="noAlignment" type="text" selector="(//div[@data-role='video']//iframe)[{{arg1}}][not(contains(@style,'text-align:'))]" parameterized="true"/>
        <element name="border" type="text" selector="(//div[@data-role='video']//iframe)[{{arg1}}][contains(@style,'border-style: {{arg2}};')]" parameterized="true"/>
        <element name="borderColor" type="text" selector="(//div[@data-role='video']//iframe)[{{arg1}}][contains(@style,'border-color: {{arg2}};')]" parameterized="true"/>
        <element name="noBorderColor" type="text" selector="(//div[@data-role='video']//iframe)[{{arg1}}][not(contains(@style,'border-color:'))]" parameterized="true"/>
        <element name="borderWidth" type="text" selector="(//div[@data-role='video']//iframe)[{{arg1}}][contains(@style,'border-width: {{arg2}}px;')]" parameterized="true"/>
        <element name="borderRadius" type="text" selector="(//div[@data-role='video']//iframe)[{{arg1}}][contains(@style,'border-radius: {{arg2}}px;')]" parameterized="true"/>
        <element name="cssClasses" type="text" selector="(//div[@data-role='video']//iframe)[{{arg1}}][@class='{{arg2}}']" parameterized="true"/>
        <element name="noCssClasses" type="text" selector="(//div[@data-role='video']//iframe)[{{arg1}}][not(@class)]" parameterized="true"/>
        <element name="margins" type="text" selector="(//div[@data-role='video']//iframe)[{{arg1}}][contains(@style,'margin: {{arg2}}px;')]" parameterized="true"/>
        <element name="padding" type="text" selector="(//div[@data-role='video']//iframe)[{{arg1}}][contains(@style,'padding: {{arg2}}px;')]" parameterized="true"/>
    </section>
</sections>
