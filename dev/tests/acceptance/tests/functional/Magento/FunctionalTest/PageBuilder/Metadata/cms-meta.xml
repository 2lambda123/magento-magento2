<operations xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="../../../../../../vendor/magento/magento2-functional-testing-framework/src/Magento/FunctionalTestingFramework/DataGenerator/etc/dataOperation.xsd">
    <operation name="ConfigCMS" dataType="config" type="create"
               auth="adminFormKey" url="/admin/system_config/save/section/cms/" method="POST" successRegex="/messages-message-success/" returnRegex="" >
        <contentType>application/x-www-form-urlencoded</contentType>
        <field key="config_state[cms_wysiwyg]">boolean</field>
        <field key="groups[wysiwyg][fields][enabled][value]">string</field>
        <field key="groups[wysiwyg][fields][editor][inherit]">boolean</field>
        <field key="groups[wysiwyg][fields][use_static_urls_in_catalog][value]">boolean</field>
        <field key="config_state[cms_pagebuilder]">boolean</field>
        <field key="groups[pagebuilder][fields][enabled][value]">boolean</field>
    </operation>
</operations>
