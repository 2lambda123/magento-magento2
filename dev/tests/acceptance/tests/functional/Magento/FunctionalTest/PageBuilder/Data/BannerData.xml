<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="../../../../../../vendor/magento/magento2-functional-testing-framework/src/Magento/FunctionalTestingFramework/DataGenerator/etc/dataProfileSchema.xsd">
    <entity name="PageBuilderBannerImageProperty" type="pagebuilder_banner_image_property">
        <data key="name">Image</data>
        <data key="section">appearance</data>
        <data key="fieldName">image</data>
        <data key="value">magento.jpg</data>
        <data key="fileType">jpg</data>
        <data key="path">wysiwyg/pagebuilder/m/a/magento</data>
    </entity>
    <entity name="PageBuilderBannerMobileImageProperty" type="pagebuilder_banner_mobileImage_property">
        <data key="name">Mobile Image</data>
        <data key="section">appearance</data>
        <data key="fieldName">mobile_image</data>
        <data key="value">magento2.jpg</data>
        <data key="fileType">jpg</data>
        <data key="path">wysiwyg/pagebuilder/m/a/magento2</data>
    </entity>
    <entity name="PageBuilderBannerBackgroundSizeProperty" type="pagebuilder_banner_backgroundSize_property">
        <data key="name">Background Size</data>
        <data key="section">appearance</data>
        <data key="fieldName">background_size</data>
        <data key="value">cover</data>
    </entity>
    <entity name="PageBuilderBannerShowButtonProperty" type="pagebuilder_banner_showButton_property">
        <data key="name">Show Button</data>
        <data key="section">appearance</data>
        <data key="fieldName">show_button</data>
        <data key="value">always</data>
    </entity>
    <entity name="PageBuilderBannerShowContentOverlayProperty" type="pagebuilder_banner_showContentOverlay_property">
        <data key="name">Show Content Overlay</data>
        <data key="section">appearance</data>
        <data key="fieldName">show_content_overlay</data>
        <data key="value">always</data>
    </entity>
    <entity name="PageBuilderBannerContentOverlayColorProperty" type="pagebuilder_banner_contentOverlayColor_property">
        <data key="name">Content Overlay Color</data>
        <data key="section">appearance</data>
        <data key="fieldName">content_overlay_color</data>
        <data key="value">0d0d0d</data>
    </entity>
    <entity name="PageBuilderBannerOverlayTransparencyProperty" type="pagebuilder_banner_overlayTransparency_property">
        <data key="name">Overlay Transparency</data>
        <data key="section">appearance</data>
        <data key="fieldName">overlay_transparency</data>
        <data key="value">50</data>
    </entity>
    <entity name="PageBuilderBannerMinimumHeightProperty" type="pagebuilder_banner_minimumHeight_property">
        <data key="name">Minimum Height</data>
        <data key="section">appearance</data>
        <data key="fieldName">minimum_height</data>
        <data key="value">500</data>
    </entity>
    <entity name="PageBuilderBannerMessageProperty" type="pagebuilder_banner_message_property">
        <data key="name">Message</data>
        <data key="section">content</data>
        <data key="role">textarea</data>
        <data key="fieldName">message</data>
        <data key="cmsPageLinkName" unique="suffix">cms-page-link-</data>
        <data key="value">Good Night!</data>
    </entity>
    <entity name="PageBuilderBannerButtonTextProperty" type="pagebuilder_banner_buttonText_property">
        <data key="name">Button Text</data>
        <data key="section">content</data>
        <data key="fieldName">button_text</data>
        <data key="value">Good Morning!</data>
    </entity>
    <entity name="PageBuilderBannerLinkUrlProperty" type="pagebuilder_banner_linkUrl_property">
        <data key="name">Link URL</data>
        <data key="section">content</data>
        <data key="fieldName">link_url</data>
        <data key="value">www.magento.com</data>
    </entity>
    <entity name="PageBuilderBannerNewTabProperty" type="pagebuilder_newTab_property">
        <data key="name">Open in new tab</data>
        <data key="section">content</data>
        <data key="fieldName">open_in_new_tab</data>
        <data key="value">1</data>
    </entity>
</config>
