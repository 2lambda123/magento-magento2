<sections xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:noNamespaceSchemaLocation="../../../../../../vendor/magento/magento2-functional-testing-framework/src/Magento/FunctionalTestingFramework/Page/etc/SectionObject.xsd">
    <section name="MapOnStage">
        <element name="default" type="button" selector="(//div[contains(@class,'pagebuilder-map')])[{{arg1}}]//iframe[@style='border: {{arg2}}; border-radius: {{arg3}}px; margin: {{arg4}}px; padding: {{arg5}}px; width: {{arg6}}; height: {{arg7}};']" parameterized="true"/>
        <element name="emptyMap" type="button" selector="//div[contains(@class, 'pagebuilder-map')][1]/*[contains(@src, 'center={{arg1}}') and contains(@src,',{{arg2}}')]" parameterized="true"/>
        <element name="map" type="button" selector="//div[contains(@class, 'pagebuilder-map')][1]"/>
        <element name="mapByIndex" type="button" selector="(//div[contains(@class, 'pagebuilder-map')])[{{arg1}}]" parameterized="true"/>
        <element name="mapAlignment" type="button" selector="//*[contains(@style, 'text-align: {{arg1}};')]" parameterized="true"/>
        <element name="mapNoAlignment" type="button" selector="//iframe[contains(@data-bind,'getMapUrl()') and not(contains(@style, 'text-align:'))]"/>
        <element name="mapAll" type="input" selector="(//div[contains(@class, 'pagebuilder-map')]//iframe[contains(@src,'zoom={{arg1}}') and contains(@style,'width: {{arg2}};') and contains(@style,'height: {{arg3}};') and contains(@style,'text-align: {{arg4}};') and contains(@style,'margin: {{arg5}}px;') and contains(@style,'padding: {{arg6}}px;')])[{{arg7}}]" parameterized="true"/>
        <element name="mapGetIFrameJS" type="button" selector="[data-bind=\'attr: { src: preview.getMapUrl() }, style: preview.previewStyle()\']"/>
        <element name="mapHeight" type="button" selector="//*[contains(@style, 'height: {{arg1}};')]" parameterized="true"/>
        <element name="mapMargin" type="button" selector="//*[contains(@style, 'margin: {{arg1}}px;')]" parameterized="true"/>
        <element name="mapPadding" type="button" selector="//*[contains(@style, 'padding: {{arg1}}px;')]" parameterized="true"/>
        <element name="mapPinLocation" type="button" selector="//div[@id='mapDiv']//div[contains(@style,'position: absolute; z-index: 1; transform: matrix(1, 0, 0, 1, -')]"/>
        <element name="mapProfileButton" type="button" selector="//div[@class='profile-photo profile-photo-light']//a|//div[@jsaction='loginControl.login']//a"/>
        <element name="mapSatelliteView" type="button" selector="//div[@title='Show satellite imagery']"/>
        <element name="mapToolTip" type="button" selector="//div[@class='place-card place-card-large']"/>
        <element name="mapToolTipCoordinates" type="button" selector="//div[@class='place-card place-card-large']//div[@class='place-name' and contains(.,'{{arg1}}') and contains(.,'N {{arg2}}') and contains(.,'W')]|//div[@class='place-card place-card-large']//div[@class='place-name' and contains(.,'{{arg1}}') and contains(.,'N {{arg3}}') and contains(.,'W')]" parameterized="true"/>
        <element name="mapToolTipDirectionsButton" type="button" selector="//div[@class='place-card place-card-large']//div[@jsaction='placeCard.directions']//a"/>
        <element name="mapToolTipSaveButton" type="button" selector="//div[@class='place-card place-card-large']//div[@jsaction='placeCard.star']"/>
        <element name="mapToolTipViewLargerMapButton" type="button" selector="//div[@class='place-card place-card-large']//div[@class='google-maps-link']//a"/>
        <element name="mapViewLargerMapButton" type="button" selector="//a[.='View larger map']"/>
        <element name="mapWidth" type="button" selector="//*[contains(@style, 'width: {{arg1}};')]" parameterized="true"/>
        <element name="mapWithPin" type="button" selector="//div[contains(@class, 'pagebuilder-map')][1]/*[contains(@src, 'q={{arg1}}') and contains(@src,',{{arg2}}')]" parameterized="true"/>
        <element name="mapZoomIn" type="button" selector="//button[@title='Zoom in']"/>
        <element name="mapZoomOut" type="button" selector="//button[@title='Zoom out']"/>
    </section>
    <section name="MapOnFrontend">
        <element name="emptyMap" type="button" selector="//*[@data-role='map'][1][not(@src) and contains(@style, 'display: none;')]"/>
        <element name="map" type="button" selector="//*[@data-role='map'][1]"/>
        <element name="mapAll" type="input" selector="(//*[@data-role='map' and contains(@src,'zoom={{arg1}}') and contains(@style,'width: {{arg2}};') and contains(@style,'height: {{arg3}};') and contains(@style,'text-align: {{arg4}};') and contains(@style,'margin: {{arg5}}px;') and contains(@style,'padding: {{arg6}}px;')])[{{arg7}}]" parameterized="true"/>
        <element name="mapAlignment" type="button" selector="//*[@data-role='map'][1][contains(@style, 'text-align: {{arg1}};')]" parameterized="true"/>
        <element name="mapNoAlignment" type="button" selector="//*[@data-role='map'][not(contains(@style, 'text-align:'))]"/>
        <element name="mapCssClass" type="button" selector="//*[@data-role='map'][1][contains(@class, '{{arg1}}')]" parameterized="true"/>
        <element name="mapGetIFrameJS" type="button" selector="[data-role=\'map\']"/>
        <element name="mapHeight" type="button" selector="//*[@data-role='map'][1][contains(@style, 'height: {{arg1}};')]" parameterized="true"/>
        <element name="mapMargin" type="button" selector="//*[@data-role='map'][1][contains(@style, 'margin: {{arg1}}px;')]" parameterized="true"/>
        <element name="mapPadding" type="button" selector="//*[@data-role='map'][1][contains(@style, 'padding: {{arg1}}px;')]" parameterized="true"/>
        <element name="mapPinLocation" type="button" selector="//div[@id='mapDiv']//div[contains(@style,'position: absolute; z-index: 1; transform: matrix(1, 0, 0, 1, -')]"/>
        <element name="mapProfileButton" type="button" selector="//div[@class='profile-photo profile-photo-light']"/>
        <element name="mapSatelliteView" type="button" selector="//div[@title='Show satellite imagery']"/>
        <element name="mapToolTip" type="button" selector="//div[@class='place-card place-card-large']"/>
        <element name="mapToolTipCoordinates" type="button" selector="//div[@class='place-card place-card-large']//div[@class='place-name' and contains(.,'{{arg1}}') and contains(.,'N {{arg2}}') and contains(.,'W')]|//div[@class='place-card place-card-large']//div[@class='place-name' and contains(.,'{{arg1}}') and contains(.,'N {{arg3}}') and contains(.,'W')]" parameterized="true"/>
        <element name="mapToolTipDirectionsButton" type="button" selector="//div[@class='place-card place-card-large']//div[@jsaction='placeCard.directions']//a"/>
        <element name="mapToolTipSaveButton" type="button" selector="//div[@class='place-card place-card-large']//div[@jsaction='placeCard.star']"/>
        <element name="mapToolTipViewLargerMapButton" type="button" selector="//div[@class='place-card place-card-large']//div[@class='google-maps-link']//a"/>
        <element name="mapWidth" type="button" selector="//*[@data-role='map'][1][contains(@style, 'width: {{arg1}};')]" parameterized="true"/>
        <element name="mapWithPin" type="button" selector="//*[@data-role='map'][1][contains(@src, 'q={{arg1}}') and contains(@src,',{{arg2}}')]" parameterized="true"/>
        <element name="mapZoom" type="button" selector="//*[@data-role='map'][1][contains(@src, 'zoom={{arg1}}')]" parameterized="true"/>
        <element name="mapZoomIn" type="button" selector="//button[@title='Zoom in']"/>
        <element name="mapZoomOut" type="button" selector="//button[@title='Zoom out']"/>
    </section>
    <section name="MapOnConfigurationPanel">
        <element name="map" type="button" selector="//div[@data-index='position']//div[@class='map-field']//iframe"/>
        <element name="mapAddPin" type="button" selector="//div[@data-index='position']//div[@class='map-field']//div[contains(@style,'z-index: 1') and contains(@style,'height: 100%;') and contains(@style,'width: 100%;')]"/>
        <element name="mapFullScreen" type="button" selector="//div[@data-index='position']//button[@title='Toggle fullscreen view']"/>
        <element name="mapHelperText" type="button" selector="//div[@data-index='position']//span[.='You can move the pin by clicking or dragging it where you want. To alter the zoom please use the controls.']"/>
        <element name="mapPegman" type="button" selector="//div[@data-index='position']//div[@aria-label='Street View Pegman Control' and not(contains(@style,'visibility: hidden;'))]"/>
        <element name="mapPin" type="button" selector="//div[@data-index='position']//div[@class='map-field']//img[contains(@src,'spotlight-poi2_hdpi.png')]|//div[@data-index='position']//div[@class='map-field']//img[contains(@src,'spotlight-poi2.png')]"/>
        <element name="mapPinPosition" type="button" selector="//div[@data-index='position']//div[@class='map-field']//div[contains(@style,'left: {{arg1}}px; top: {{arg2}}px; z-index: {{arg3}};')]|//div[@data-index='position']//div[@class='map-field']//div[contains(@style,'left: {{arg4}}px; top: {{arg2}}px; z-index: {{arg3}};')]" parameterized="true"/>
        <element name="mapPosition" type="input" selector="//div[@data-index='position']//input"/>
        <element name="mapPositionJS" type="input" selector=".admin__field-map input[type=\'text\']"/>
        <element name="mapSatelliteView" type="button" selector="//div[@data-index='position']//div[@title='Show satellite imagery']"/>
        <element name="mapStreetView" type="button" selector="//div[@data-index='position']//div[@title='Show street map']"/>
        <element name="mapZoomIn" type="button" selector="//div[@data-index='position']//button[@title='Zoom in']"/>
        <element name="mapZoomOut" type="button" selector="//div[@data-index='position']//button[@title='Zoom out']"/>
    </section>
</sections>
