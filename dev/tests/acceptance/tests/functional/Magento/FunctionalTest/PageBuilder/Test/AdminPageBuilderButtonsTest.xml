        <click selector="{{AdvancedForm.advancedSectionName}}" stepKey="clickAdvancedFormSection3"/>
        <actionGroup ref="seeOptionIsSelectedSlideOutProperty" stepKey="seeAdvancedAlignmentPropertyDefault">
