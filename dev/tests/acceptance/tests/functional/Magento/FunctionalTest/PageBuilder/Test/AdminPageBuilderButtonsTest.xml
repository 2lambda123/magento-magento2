        <amOnPage url="{{CmsPagesPage.url}}" stepKey="onCmsPageGrid"/>
        <waitForPageLoad stepKey="waitForCmsPagesGridLoad"/>
        <waitForElementNotVisible selector="{{CmsPageGrid.spinner}}" stepKey="waitForSpinnerNotVisible1" />
