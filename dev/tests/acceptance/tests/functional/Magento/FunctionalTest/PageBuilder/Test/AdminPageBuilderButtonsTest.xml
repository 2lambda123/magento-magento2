        <seeElementInDOM selector="{{ButtonsBackendSection.paddingStyle(PageBuilderAdvancedMarginsAndPaddingProperty.paddingTop)}}" stepKey="seePaddingSet"/>
        <seeElement selector="{{ButtonsBackendSection.buttonItemElement(PageBuilderButtonItemTypeSecondaryProperty.value, PageBuilderButtonItemTextProperty.value)}}" stepKey="seeButton2"/>
        <amOnPage url="buttons{{PageBuilderPageTitle.pageName}}" stepKey="amOnPageBuilderPage2"/>
        <seeElement selector="{{ButtonsFrontendSection.buttonItemElement(PageBuilderButtonItemTypePrimaryProperty.value, PageBuilderButtonItemLinkProperty.value, PageBuilderButtonItemTextProperty.value)}}" stepKey="seePrimaryButtonOnStorefront"/>
        <seeElementInDOM selector="{{ButtonsFrontendSection.alignmentStyle(PageBuilderAdvancedAlignmentProperty.value)}}" stepKey="seeAlignmentSetOnStorefront"/>
        <seeElementInDOM selector="{{ButtonsFrontendSection.borderStyle(PageBuilderAdvancedBorderSizeProperty10.value, PageBuilderAdvancedBorderPropertyDotted.value, PageBuilderAdvancedBorderColorProperty_ffffff.rbg)}}" stepKey="seeBorderSetOnStorefront"/>
        <seeElementInDOM selector="{{ButtonsFrontendSection.borderRadiusStyle(PageBuilderAdvancedBorderRadiusProperty12.value)}}" stepKey="seeBorderRadiusSetOnStorefront"/>
        <seeElementInDOM selector="{{ButtonsFrontendSection.marginStyle(PageBuilderAdvancedMarginsAndPaddingProperty.marginTop)}}" stepKey="seeMarginSetOnStorefront"/>
        <seeElementInDOM selector="{{ButtonsFrontendSection.paddingStyle(PageBuilderAdvancedMarginsAndPaddingProperty.paddingTop)}}" stepKey="seePaddingSetOnStorefront"/>
        <seeElementInDOM selector="{{ButtonsFrontendSection.class(PageBuilderAdvancedCssClassesProperty.value)}}" stepKey="seeAdvancedCssClasses"/>
        <seeElement selector="{{ButtonsFrontendSection.buttonItemElement(PageBuilderButtonItemTypeSecondaryProperty.value, PageBuilderButtonItemLinkProperty.value, PageBuilderButtonItemTextProperty.value)}}" stepKey="seeSecondaryButtonOnStorefront"/>
        <!-- Check fields and edit advanced section back to default -->
        <amOnPage url="{{CmsPagesPage.url}}" stepKey="onCmsPageGrid2"/>
        <waitForPageLoad stepKey="waitForCmsPagesGridLoad2"/>
        <click selector="{{CmsPagesPageActionsSection.FilterBtn}}" stepKey="clickFiltersBtn2" />
        <fillField selector="{{CmsPagesPageActionsSection.URLKey}}" userInput="buttons{{PageBuilderPageTitle.pageName}}" stepKey="fillOutURLKey2" />
        <click selector="{{CmsPagesPageActionsSection.ApplyFiltersBtn}}" stepKey="clickApplyBtn2" />
        <waitForPageLoad stepKey="waitForSearchResults2" />
        <waitForElementVisible selector="{{CmsPageGrid.firstItemSelectButton}}" stepKey="waitForFirstItemButton2" />
        <click selector="{{CmsPageGrid.firstItemSelectButton}}" stepKey="clickSelectButtonOnFirstItem2"/>
        <waitForElementVisible selector="{{CmsPageGrid.firstItemEditButton}}" stepKey="waitForFirstItemEditButton2" />
        <click selector="{{CmsPageGrid.firstItemEditButton}}" stepKey="clickSelectEditOnFirstItem2"/>
        <waitForPageLoad stepKey="waitForEditCmsPage2" />
