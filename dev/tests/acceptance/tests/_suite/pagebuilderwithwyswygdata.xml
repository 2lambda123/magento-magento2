<suites xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../vendor/magento/magento2-functional-testing-framework/src/Magento/FunctionalTestingFramework/Suite/etc/suiteSchema.xsd">
    <suite name="pagebuilderwithwysiwygdata">
        <before>
            <createData stepKey="enablePageBuilderAndWYSIWYG" entity="enablePageBuilderAndWYSIWYGForm"/>
        </before>
        <after>
            <createData stepKey="DisabledWYSIWYG" entity="disableWYSYWYGForm"/>
        </after>
        <include>
            <!--<module name="PageBuilder" />-->
            <group name="pagebuilder-tabs" />
            <group name="pagebuilder-tabs-content"/>
            <group name="pagebuilder-accordion"/>
            <group name="pagebuilder-advancedConfig"/>
            <group name="pagebuilder-anchor"/>
            <group name="pagebuilder-buttons"/>
            <group name="pagebuilder-column"/>
            <group name="pagebuilder-row"/>
            <group name="pagebuilder-text"/>
            <group name="pagebuilder-urlInput"/>
            <group name="pagebuilder-video"/>
        </include>
        <exclude>
            <group name="pagebuilder-customBeforeAfter"/>
        </exclude>
    </suite>
</suites>
